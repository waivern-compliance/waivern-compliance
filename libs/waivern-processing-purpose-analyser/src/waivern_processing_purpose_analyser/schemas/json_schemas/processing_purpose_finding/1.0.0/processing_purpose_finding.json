{
  "$defs": {
    "BaseAnalysisOutputMetadata": {
      "additionalProperties": true,
      "description": "Base metadata for analysis outputs.",
      "properties": {
        "ruleset_used": {
          "description": "Name of the ruleset used for analysis",
          "title": "Ruleset Used",
          "type": "string"
        },
        "llm_validation_enabled": {
          "description": "Whether LLM validation was enabled for this analysis",
          "title": "Llm Validation Enabled",
          "type": "boolean"
        },
        "analysis_timestamp": {
          "description": "ISO 8601 timestamp when the analysis was performed",
          "title": "Analysis Timestamp",
          "type": "string"
        },
        "evidence_context_size": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Size of context used for evidence extraction",
          "title": "Evidence Context Size"
        }
      },
      "required": [
        "ruleset_used",
        "llm_validation_enabled"
      ],
      "title": "BaseAnalysisOutputMetadata",
      "type": "object"
    },
    "BaseFindingEvidence": {
      "description": "Evidence item with content and collection timestamp.",
      "properties": {
        "content": {
          "description": "The evidence content snippet",
          "title": "Content",
          "type": "string"
        },
        "collection_timestamp": {
          "description": "When the evidence was collected",
          "title": "Collection Timestamp",
          "type": "string"
        }
      },
      "required": [
        "content"
      ],
      "title": "BaseFindingEvidence",
      "type": "object"
    },
    "JsonValue": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "integer"
        },
        {
          "type": "number"
        },
        {
          "type": "boolean"
        },
        {
          "items": {
            "$ref": "#/$defs/JsonValue"
          },
          "type": "array"
        },
        {
          "additionalProperties": {
            "$ref": "#/$defs/JsonValue"
          },
          "type": "object"
        },
        {
          "type": "null"
        }
      ]
    },
    "ProcessingPurposeFindingMetadata": {
      "additionalProperties": false,
      "description": "Metadata for processing purpose findings.\n\nExtends BaseFindingMetadata which provides:\n- source: str - Source file or location where the data was found\n- context: dict[str, object] - Extensible context for pipeline metadata",
      "properties": {
        "source": {
          "description": "Source file or location where the data was found",
          "title": "Source",
          "type": "string"
        },
        "context": {
          "additionalProperties": {
            "$ref": "#/$defs/JsonValue"
          },
          "description": "Extensible context for pipeline metadata (connector_type, artifact_id, etc.)",
          "title": "Context",
          "type": "object"
        },
        "line_number": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Line number where the finding was detected (for source code findings)",
          "title": "Line Number"
        }
      },
      "required": [
        "source"
      ],
      "title": "ProcessingPurposeFindingMetadata",
      "type": "object"
    },
    "ProcessingPurposeFindingModel": {
      "description": "Processing purpose finding structure.",
      "properties": {
        "id": {
          "description": "Unique identifier for this finding (UUID)",
          "title": "Id",
          "type": "string"
        },
        "evidence": {
          "description": "Evidence items with content and timestamps for this finding",
          "items": {
            "$ref": "#/$defs/BaseFindingEvidence"
          },
          "minItems": 1,
          "title": "Evidence",
          "type": "array"
        },
        "matched_patterns": {
          "description": "Patterns that were matched during analysis",
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "title": "Matched Patterns",
          "type": "array"
        },
        "purpose": {
          "description": "Processing purpose name",
          "title": "Purpose",
          "type": "string"
        },
        "purpose_category": {
          "default": "",
          "description": "Category of the processing purpose",
          "title": "Purpose Category",
          "type": "string"
        },
        "metadata": {
          "anyOf": [
            {
              "$ref": "#/$defs/ProcessingPurposeFindingMetadata"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Additional metadata about the finding"
        },
        "service_category": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Service category from ServiceIntegrationRule (when applicable)",
          "title": "Service Category"
        },
        "collection_type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Collection type from DataCollectionRule (when applicable)",
          "title": "Collection Type"
        },
        "data_source": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Data source from DataCollectionRule (when applicable)",
          "title": "Data Source"
        }
      },
      "required": [
        "evidence",
        "matched_patterns",
        "purpose"
      ],
      "title": "ProcessingPurposeFindingModel",
      "type": "object"
    },
    "ProcessingPurposeSummary": {
      "description": "Summary statistics for processing purpose findings.",
      "properties": {
        "total_findings": {
          "description": "Total number of processing purpose findings",
          "minimum": 0,
          "title": "Total Findings",
          "type": "integer"
        },
        "purposes_identified": {
          "description": "Number of unique processing purposes identified",
          "minimum": 0,
          "title": "Purposes Identified",
          "type": "integer"
        },
        "purpose_categories": {
          "additionalProperties": {
            "type": "integer"
          },
          "description": "Count of findings by purpose category",
          "title": "Purpose Categories",
          "type": "object"
        },
        "purposes": {
          "description": "Per-purpose breakdown of findings",
          "items": {
            "$ref": "#/$defs/PurposeBreakdown"
          },
          "title": "Purposes",
          "type": "array"
        }
      },
      "required": [
        "total_findings",
        "purposes_identified",
        "purpose_categories"
      ],
      "title": "ProcessingPurposeSummary",
      "type": "object"
    },
    "PurposeBreakdown": {
      "description": "Per-purpose breakdown of findings.",
      "properties": {
        "purpose": {
          "description": "Purpose name",
          "title": "Purpose",
          "type": "string"
        },
        "findings_count": {
          "description": "Number of findings for this purpose",
          "minimum": 0,
          "title": "Findings Count",
          "type": "integer"
        }
      },
      "required": [
        "purpose",
        "findings_count"
      ],
      "title": "PurposeBreakdown",
      "type": "object"
    }
  },
  "description": "Complete output structure for processing_purpose_finding schema.\n\nThis model represents the full wire format for processing purpose analysis results,\nincluding findings, summary statistics, and analysis metadata.",
  "properties": {
    "findings": {
      "description": "List of processing purpose findings from GDPR compliance analysis",
      "items": {
        "$ref": "#/$defs/ProcessingPurposeFindingModel"
      },
      "title": "Findings",
      "type": "array"
    },
    "summary": {
      "$ref": "#/$defs/ProcessingPurposeSummary",
      "description": "Summary statistics of the processing purpose analysis"
    },
    "analysis_metadata": {
      "$ref": "#/$defs/BaseAnalysisOutputMetadata",
      "description": "Metadata about the analysis process"
    }
  },
  "required": [
    "findings",
    "summary",
    "analysis_metadata"
  ],
  "title": "ProcessingPurposeFindingOutput",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "version": "1.0.0"
}

{
  "$defs": {
    "SourceCodeAnalysisMetadataModel": {
      "description": "Pydantic model for source code analysis metadata.",
      "properties": {
        "total_files": {
          "title": "Total Files",
          "type": "integer"
        },
        "total_lines": {
          "title": "Total Lines",
          "type": "integer"
        },
        "analysis_timestamp": {
          "title": "Analysis Timestamp",
          "type": "string"
        }
      },
      "required": [
        "total_files",
        "total_lines",
        "analysis_timestamp"
      ],
      "title": "SourceCodeAnalysisMetadataModel",
      "type": "object"
    },
    "SourceCodeClassModel": {
      "description": "Pydantic model for source code classes.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "line_start": {
          "title": "Line Start",
          "type": "integer"
        },
        "line_end": {
          "title": "Line End",
          "type": "integer"
        },
        "extends": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Extends"
        },
        "implements": {
          "items": {
            "type": "string"
          },
          "title": "Implements",
          "type": "array"
        },
        "properties": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/SourceCodeClassPropertyModel"
          },
          "title": "Properties"
        },
        "methods": {
          "items": {
            "$ref": "#/$defs/SourceCodeFunctionModel"
          },
          "title": "Methods",
          "type": "array"
        }
      },
      "required": [
        "name",
        "line_start",
        "line_end"
      ],
      "title": "SourceCodeClassModel",
      "type": "object"
    },
    "SourceCodeClassPropertyModel": {
      "description": "Pydantic model for source code class properties.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Type"
        },
        "visibility": {
          "default": "public",
          "title": "Visibility",
          "type": "string"
        },
        "is_static": {
          "default": false,
          "title": "Is Static",
          "type": "boolean"
        },
        "default_value": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Default Value"
        }
      },
      "required": [
        "name"
      ],
      "title": "SourceCodeClassPropertyModel",
      "type": "object"
    },
    "SourceCodeFileDataModel": {
      "description": "Pydantic model for source code file data (structural extraction only).",
      "properties": {
        "file_path": {
          "title": "File Path",
          "type": "string"
        },
        "language": {
          "title": "Language",
          "type": "string"
        },
        "raw_content": {
          "title": "Raw Content",
          "type": "string"
        },
        "metadata": {
          "$ref": "#/$defs/SourceCodeFileMetadataModel"
        },
        "functions": {
          "items": {
            "$ref": "#/$defs/SourceCodeFunctionModel"
          },
          "title": "Functions",
          "type": "array"
        },
        "classes": {
          "items": {
            "$ref": "#/$defs/SourceCodeClassModel"
          },
          "title": "Classes",
          "type": "array"
        }
      },
      "required": [
        "file_path",
        "language",
        "raw_content",
        "metadata"
      ],
      "title": "SourceCodeFileDataModel",
      "type": "object"
    },
    "SourceCodeFileMetadataModel": {
      "description": "Pydantic model for source code file metadata.",
      "properties": {
        "file_size": {
          "title": "File Size",
          "type": "integer"
        },
        "line_count": {
          "title": "Line Count",
          "type": "integer"
        },
        "last_modified": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Last Modified"
        }
      },
      "required": [
        "file_size",
        "line_count"
      ],
      "title": "SourceCodeFileMetadataModel",
      "type": "object"
    },
    "SourceCodeFunctionModel": {
      "description": "Pydantic model for source code functions.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "line_start": {
          "title": "Line Start",
          "type": "integer"
        },
        "line_end": {
          "title": "Line End",
          "type": "integer"
        },
        "parameters": {
          "items": {
            "$ref": "#/$defs/SourceCodeFunctionParameterModel"
          },
          "title": "Parameters",
          "type": "array"
        },
        "return_type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Return Type"
        },
        "visibility": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Visibility"
        },
        "is_static": {
          "default": false,
          "title": "Is Static",
          "type": "boolean"
        },
        "docstring": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Docstring"
        }
      },
      "required": [
        "name",
        "line_start",
        "line_end"
      ],
      "title": "SourceCodeFunctionModel",
      "type": "object"
    },
    "SourceCodeFunctionParameterModel": {
      "description": "Pydantic model for source code function parameters.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Type"
        },
        "default_value": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Default Value"
        }
      },
      "required": [
        "name"
      ],
      "title": "SourceCodeFunctionParameterModel",
      "type": "object"
    }
  },
  "description": "Pydantic model for complete source code analysis data.\n\nThis model represents the full wire format for source code analysis results.\nExtends BaseSchemaOutput for schema generation support.",
  "properties": {
    "schemaVersion": {
      "description": "Schema version identifier",
      "title": "Schemaversion",
      "type": "string"
    },
    "name": {
      "description": "Name of the source code analysis",
      "title": "Name",
      "type": "string"
    },
    "description": {
      "description": "Description of the analysis",
      "title": "Description",
      "type": "string"
    },
    "language": {
      "description": "Primary programming language",
      "title": "Language",
      "type": "string"
    },
    "source": {
      "description": "Source path or identifier",
      "title": "Source",
      "type": "string"
    },
    "metadata": {
      "$ref": "#/$defs/SourceCodeAnalysisMetadataModel",
      "description": "Analysis metadata including file counts and timestamp"
    },
    "data": {
      "description": "List of analysed source code files",
      "items": {
        "$ref": "#/$defs/SourceCodeFileDataModel"
      },
      "title": "Data",
      "type": "array"
    }
  },
  "required": [
    "schemaVersion",
    "name",
    "description",
    "language",
    "source",
    "metadata",
    "data"
  ],
  "title": "SourceCodeDataModel",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "version": "1.0.0"
}
